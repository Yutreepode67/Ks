-- Create main draggable frame local ScreenGui = script.Parent local Frame = Instance.new("Frame") Frame.Size = UDim2.new(0, 250, 0, 180) Frame.Position = UDim2.new(0.5, -125, 0.5, -90) Frame.BackgroundColor3 = Color3.fromRGB(40, 40, 40) -- Frame color Frame.BorderSizePixel = 0 Frame.Active = true Frame.Draggable = true Frame.Parent = ScreenGui -- UI Corner local UICorner = Instance.new("UICorner", Frame) -- Title local Title = Instance.new("TextLabel") Title.Size = UDim2.new(1, 0, 0, 30) Title.BackgroundTransparency = 1 Title.Text = "Script Hub" Title.Font = Enum.Font.GothamBold Title.TextSize = 20 Title.TextColor3 = Color3.fromRGB(255, 255, 255) -- Title text color Title.Parent = Frame -- Button creator function local function createButton(name, yPos, scriptUrl) local button = Instance.new("TextButton") button.Size = UDim2.new(0.8, 0, 0, 35) button.Position = UDim2.new(0.1, 0, 0, yPos) button.BackgroundColor3 = Color3.fromRGB(60, 60, 60) -- Button color button.Text = name button.Font = Enum.Font.Gotham button.TextColor3 = Color3.fromRGB(255, 255, 255) -- Button text color button.TextSize = 16 button.Parent = Frame local corner = Instance.new("UICorner", button) button.MouseButton1Click:Connect(function() loadstring(game:HttpGet(scriptUrl))() end) end -- Add buttons createButton("Super Gui", 40, "https://pastefy.app/agqEB2qs/raw") createButton("topk3k", 80, "https://pastebin.com/raw/6qaaCgMs") createButton("sheldoni", 120, "https://raw.githubusercontent.com/ejabar65/sheldoni/refs/heads/main/README.md") 
